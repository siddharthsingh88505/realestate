                            <li><a href="/contact">CONTACT</a></li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
        <div class="togglebars container">
            <ul class="toggle-menu">
                <li><a href="/">HOME</a></li>
                <li><a href="/featured">FEATURED LISTING</a></li>
                <li><a href="/about">ABOUT US</a></li>
                <li><a href="/contact">CONTACT</a></li>
            </ul>
        </div>
    </header>

    <!-- register section -->
    <section class="container form-section">
        <h2 class="text-center form-title">Register as Agent</h2>
        <!-- failed error message -->
        <% if(status==="Failed" ) { %>
            <div class="alert alert-danger">
                <strong>
                    <%= status %>,
                        <%= message %>
                </strong>
            </div>
            <% } %>
                <!-- success message -->
                <% if(status==="Success" ) { %>
                    <div class="alert alert-success">
                        <strong>
                            <%= status %>,
                                <%= message %>
                        </strong>
                    </div>
                    <% } %>
                        <div class="form-container">
                            <form action="/update-user" method="post">
                                <!-- email input section -->
                                <div class="input-email">
                                    <i class="fa fa-envelope email-icon"></i>
                                    <input type="email" placeholder="Email" class="email" name="email"
                                        inputmode="numeric" tabindex="1" value="<%= data.email %>">
                                </div>
                                <p class="error email-error text-danger">Email error</p>
                                <!-- password input section -->
                                <div class="input-password">
                                    <i class="fa fa-lock lock"></i>
                                    <input type="password" placeholder="Password" class="password" name="password"
                                        tabindex="2" value="<%= data.password %>">
                                </div>
                                <p class="error password-error text-danger">Password error</p>
                                <!-- submit btn -->
                                <div class="input-submit">
                                    <input type="submit" value="Update User" class="submit-btn" tabindex="3">
                                </div>

                            </form>
                        </div>
    </section>

    <!-- footer section -->
    <footer class="container-fluid footer-section">
        <div class="container">
            <div class="row">
                <div class="col-lg-4 col-md-6 footer-widget">
